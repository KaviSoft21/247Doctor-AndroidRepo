<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="au.com.twoFourSevendoctor.channelDoctor"
    android:installLocation="auto"
    android:versionCode="2"
    android:versionName="1.0.2" >

    <supports-screens
        android:anyDensity="true"
        android:compatibleWidthLimitDp="320"
        android:largeScreens="false"
        android:normalScreens="true"
        android:resizeable="true"
        android:smallScreens="true" />
    <!-- android:largeHeap="true" -->

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <!-- android:icon="@drawable/ic_launcher" icon -->
    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Light.NoTitleBar" > 

        <!--
       android:theme="@style/AppTheme"
       @android:style/Theme.Black.NoTitleBar.Fullscreen
       android:theme="@style/Theme.NoTitle"
        -->
        <activity
            android:name="au.com.twoFourSevendoctor.channelDoctor.activities.LoadingActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="au.com.twoFourSevendoctor.channelDoctor.activities.LoginActivity" >
        </activity>
        <activity android:name="au.com.twoFourSevendoctor.channelDoctor.activities.BookingActivity" >
        </activity>
        <activity android:name="au.com.twoFourSevendoctor.channelDoctor.activities.RegisterActivity" >
        </activity>
        <activity android:name="au.com.twoFourSevendoctor.channelDoctor.activities.ConfirmationActivity" >
        </activity>
        <activity android:name="au.com.twoFourSevendoctor.channelDoctor.activities.TeleconsultActivity" >
        </activity>
        <activity android:name="au.com.twoFourSevendoctor.channelDoctor.activities.CallbackActivity" >
        </activity>
    </application>

</manifest>